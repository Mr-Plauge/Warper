{
  "type": "origins:multiple",
  "hidden": true,
  "hurt_death": {
    "type": "origins:self_action_when_hit",
    "entity_action": {
      "type": "origins:if_else",
      "condition": {
        "type": "origins:health",
        "comparison": ">",
        "compare_to": 0
      },
      "if_action": {
        "type": "origins:execute_command",
        "command": "playsound minecraft:block.weeping_vines.break player @s ~ ~ ~ 1 1",
        "permission_level": 4
      },
      "else_action": {
        "type": "origins:execute_command",
        "command": "playsound minecraft:block.weeping_vines.break player @s ~ ~ ~ 1 1",
        "permission_level": 4
      }
    },
    "cooldown": 1
  },
  "spawn": {
    "type": "origins:action_on_callback",
    "entity_action_added": {
      "type": "origins:if_else",
      "condition": {
        "type": "origins:and",
        "conditions": [
          {
            "type": "origins:sneaking",
            "inverted": true
          },
          {
            "type": "origins:gamemode",
            "gamemode": "spectator",
            "inverted": true
          },
          {
            "type": "origins:status_effect",
            "effect": "minecraft:invisibility",
            "inverted": true
          }
        ]
      },
      "if_action": {
        "type": "origins:delay",
        "ticks": 5,
        "action": {
          "type": "origins:execute_command",
          "command": "playsound minecraft:ambient.warped_forest.mood player @s ~ ~ ~ 0.5 1",
          "permission_level": 4
        }
      }
    }
  },
  "ambient": {
    "type": "origins:action_over_time",
    "interval": 100,
    "entity_action": {
      "type": "origins:chance",
      "action": {
        "type": "origins:execute_command",
        "command": "playsound minecraft:ambient.warped_forest.mood player @s ~ ~ ~ 0.5 1",
        "permission_level": 4
      },
      "chance": 0.05
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:sneaking",
          "inverted": true
        },
        {
          "type": "origins:gamemode",
          "gamemode": "spectator",
          "inverted": true
        },
        {
          "type": "origins:status_effect",
          "effect": "minecraft:invisibility",
          "inverted": true
        }
      ]
    }
  }
}